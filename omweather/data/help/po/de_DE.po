#: data/help/omweather.xml.in.h:1
msgid ""
"<display_text>Add Custom Station</display_text> button is used to add "
"weather stations not found on drop down menus."
msgstr ""

#: data/help/omweather.xml.in.h:2
msgid ""
"<display_text>Add</display_text> button allows you to add new weather "
"stations, which will then be listed in the locations screen."
msgstr ""

#: data/help/omweather.xml.in.h:3
msgid ""
"<display_text>Automatically updating data after connect to Internet: </"
"display_text> selection will controls if weather data is automatically "
"updated when Internet connection is established."
msgstr ""

#: data/help/omweather.xml.in.h:4
msgid ""
"<display_text>Background color:</display_text> setting defines the "
"background color used for icons. Color selection works same way as selecting "
"font color. Background color is not selectable if transparency is used."
msgstr ""

#: data/help/omweather.xml.in.h:5
msgid ""
"<display_text>Delete</display_text> button deletes selected station from the "
"list."
msgstr ""

#: data/help/omweather.xml.in.h:6
msgid ""
"<display_text>Enable station from GPS:</display_text> check box is used to "
"enable automatic weather station selection when ever GPS is enabled."
msgstr ""

#: data/help/omweather.xml.in.h:7
msgid ""
"<display_text>Font color:</display_text> defines applets font color. "
"Touching color button will open <display_text>Pick a Color</display_text> "
"dialog where it's possible to select font color."
msgstr ""

#: data/help/omweather.xml.in.h:8
msgid ""
"<display_text>Hide arrows:</display_text> selection will controls if arrows "
"used to change weather station are shown on the applet."
msgstr ""

#: data/help/omweather.xml.in.h:9
msgid ""
"<display_text>Hide station name:</display_text> selection will controls if "
"station name are shown on the applet."
msgstr ""

#: data/help/omweather.xml.in.h:10
msgid ""
"<display_text>Icon set:</display_text> selects which icon set is used. You "
"can install more icon sets with the application manager (if Maemo extras "
"repository is configured) or going to OMWeathers Maemo garage page (http://"
"omweather.garage.maemo.org/) and downloading icon sets directly from there."
msgstr ""

#: data/help/omweather.xml.in.h:11
msgid ""
"<display_text>Icon size:</display_text> defines icon size. Size can be tiny, "
"small, medium, large or giant."
msgstr ""

#: data/help/omweather.xml.in.h:12
msgid ""
"<display_text>Interface</display_text> tab is used to configure settings "
"that controls visual appearance of the OMWeather."
msgstr ""

#: data/help/omweather.xml.in.h:13
msgid ""
"<display_text>Layout:</display_text> selects how icons are shown in the home "
"screen. Possible selections are one row, one column, two rows, two columns "
"or combinated."
msgstr ""

#: data/help/omweather.xml.in.h:14
msgid ""
"<display_text>Locations</display_text> tab lists all locations which weather "
"data OMWeather will receive when applets refresh button is touched or "
"automatic update happens."
msgstr ""

#: data/help/omweather.xml.in.h:15
msgid ""
"<display_text>Rename</display_text> button allows you to change name of the "
"selected station."
msgstr ""

#: data/help/omweather.xml.in.h:16
msgid ""
"<display_text>Show only current weather on first icon:</display_text> "
"selection will show current weather on first icon and forecast for a rest of "
"the day is shown on the next icon."
msgstr ""

#: data/help/omweather.xml.in.h:17
msgid ""
"<display_text>Swap hi/low temperature:</display_text> selection will change "
"way how lowest and highest temperature for a day is shown on the applet. If "
"selected, applet will show high temperature first."
msgstr ""

#: data/help/omweather.xml.in.h:18
msgid ""
"<display_text>Switch to the next station after:</display_text> drop down "
"menu controls time after applet changes to next weather station."
msgstr ""

#: data/help/omweather.xml.in.h:19
msgid ""
"<display_text>Transparency:</display_text> selection will show background "
"image behind the icons."
msgstr ""

#: data/help/omweather.xml.in.h:20
msgid ""
"<display_text>Units</display_text> tab is used to select temperature, "
"distance and wind speed units shown on weather data. Used units can be "
"selected from a drop down menus."
msgstr ""

#: data/help/omweather.xml.in.h:21
msgid ""
"<display_text>Up</display_text> and <display_text>down</display_text> arrows "
"allows you to change the order of the selected locations."
msgstr ""

#: data/help/omweather.xml.in.h:22
msgid ""
"<display_text>Update</display_text> tab is used to configure update interval "
"of a weather data. This tab shows also time when next update will be done."
msgstr ""

#: data/help/omweather.xml.in.h:23
msgid ""
"<display_text>Visible items:</display_text> selects how many icons will be "
"shown in the tablets home screen. Possibilities go from 1 to 5."
msgstr ""

